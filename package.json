{
  "name": "css-property-parser",
  "version": "1.0.4",
  "description": "Validate css properties and expand shorthand css properties",
  "keywords": [
    "css"
  ],
  "bugs": "https://github.com/mahirshah/css-property-parser/issues",
  "main": "src/index.js",
  "scripts": {
    "prepublish": "npm start",
    "test": "./node_modules/.bin/mocha --reporter spec --recursive",
    "start": "./updateCSSData.sh",
    "clean": "rm -rf src/grammars/generated",
    "benchmark": "node test/benchmark.js",
    "doctoc": "node ./node_modules/doctoc/doctoc.js README.md",
    "lint": "node node_modules/eslint/bin/eslint.js src/",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "test"
  ],
  "author": "mahirshah",
  "license": "(MIT AND MPL-2.0)",
  "repository": {
    "type": "git",
    "url": "https://github.com/mahirshah/css-property-parser"
  },
  "types": "css-property-parser.d.ts",
  "devDependencies": {
    "async": "^2.5.0",
    "benchmark": "^2.1.4",
    "chai": "^4.1.1",
    "css-values": "^0.1.0",
    "doctoc": "^1.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "microtime": "^2.1.6",
    "mocha": "^3.5.0",
    "pre-commit": "^1.2.2",
    "postcss-value-parser": "^3.3.0",
    "sinon": "^2.4.1",
    "mdn-data": "1.0.0"
  },
  "dependencies": {
    "fs-extra": "^3.0.1",
    "moo": "^0.4.1",
    "nearley": "^2.11.0"
  }
}
