targetCountersFunc {
  Exp = "target-counters(" ( ( string | url ) ) "," customIdent "," string "," CounterStyle? ")"
  string = ( "\"" ( ~"\"" any )+ "\"" ) | ( "'" ( ~"'" any )+ "'" )
  url = ("url(" "\"" ( ~"\"" any )+ "\"" ")") | ("url(" "'" ( ~"'" any )+ "'" ")")
  customIdent = ( alnum | "-" | "_" )+
  CounterStyle = ( CounterStyleName | "symbols()" )
  CounterStyleName = customIdent
}