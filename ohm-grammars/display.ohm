display {
  Exp = ( DisplayListitem | DisplayInternal | Display_IntermediateRule0 | DisplayLegacy | DisplayBox )
  Display_IntermediateRule0 = ( ( DisplayOutside | DisplayInside )+ )
  DisplayOutside = ( "inline" | "run-in" | "block" )
  DisplayInside = ( "flow-root" | "subgrid" | "table" | "flow" | "flex" | "grid" | "ruby" )
  DisplayListitem = ( "list-item" ( DisplayOutside? ( ( "flow-root" | "flow" ) )? ) | ( ( ( "flow-root" | "flow" ) )? DisplayOutside? ) ) | ( ( DisplayOutside? ( ( "flow-root" | "flow" ) )? ) | ( ( ( "flow-root" | "flow" ) )? DisplayOutside? ) "list-item" )
  DisplayInternal = ( "ruby-text-container" | "ruby-base-container" | "table-footer-group" | "table-header-group" | "table-column-group" | "table-row-group" | "table-caption" | "table-column" | "table-cell" | "ruby-base" | "ruby-text" | "table-row" )
  DisplayBox = ( "contents" | "none" )
  DisplayLegacy = ( "inline-list-item" | "inline-block" | "inline-table" | "inline-flex" | "inline-grid" )
}