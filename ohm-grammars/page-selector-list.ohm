pageSelectorList {
  Exp = ( listOf< PageSelector , "," > )?
  PageSelector = ( PageSelector_IntermediateRule0 | PageSelector_IntermediateRule1 )
  PageSelector_IntermediateRule0 = PseudoPage+
  PageSelector_IntermediateRule1 = ident PseudoPage*
  PseudoPage = ":" ( ( "right" | "first" | "blank" | "left" ) )
  ident = any+
}