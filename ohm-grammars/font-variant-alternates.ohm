fontVariantAlternates {
  Exp = ( FontVariantAlternates_IntermediateRule0 | "normal" )
  FontVariantAlternates_IntermediateRule5 = "annotation(" FeatureValueName ")"
  FontVariantAlternates_IntermediateRule4 = "ornaments(" FeatureValueName ( ")" | FontVariantAlternates_IntermediateRule5 )+
  FontVariantAlternates_IntermediateRule3 = "swash(" FeatureValueName ( ")" | FontVariantAlternates_IntermediateRule4 )+
  FontVariantAlternates_IntermediateRule2 = "character-variant(" listOf< FeatureValueName , "," > ( ")" | FontVariantAlternates_IntermediateRule3 )+
  FontVariantAlternates_IntermediateRule1 = "styleset(" listOf< FeatureValueName , "," > ( ")" | FontVariantAlternates_IntermediateRule2 )+
  FontVariantAlternates_IntermediateRule0 = ( "stylistic(" FeatureValueName ( ")" | "historical-forms" | FontVariantAlternates_IntermediateRule1 )+ )
  FeatureValueName = customIdent
  customIdent = ( alnum | "-" | "_" )+
}